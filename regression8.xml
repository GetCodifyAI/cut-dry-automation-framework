            <class name="com.cutanddry.qa.tests.distributor_specific.VerifySubmittingAnOrderMinimumSatisfiedOrderForAFutureDateGreaterThanItemLeadTimeItemDateInOrderTest"/>
            <class name="com.cutanddry.qa.tests.distributor_specific.VerifyVerifyImportantShipmentNoticeMessageTest"/>
            <class name="com.cutanddry.qa.tests.distributor_specific.VerifyLeadTimeItemOrderIsSplitInToTwoOrdersTest"/>
        </classes>
